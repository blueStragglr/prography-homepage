<script>
  // TODO: Change function as re-usable (scalable)
  // 2020. 02. 16. 신성환
  var x = document.getElementsByClassName('mySlides' + $('a[name=sel_th]').length);
  var sectionIndex = 5;

  function slide_section(n) {
    x = document.getElementsByClassName('mySlides' + n);
    sectionIndex = n;
    for (var i = 1; i < 6; i++) {
      if (i === n) {
        $('#' + i + 'th').css('display', 'block');
      } else {
        $('#' + i + 'th').css('display', 'none');
      }
    }
    showDivs(1);
  }

  var slideIndex = 1;
  showDivs(slideIndex);

  function plusDivs(n) {
    showDivs(slideIndex += n);
  }

  function showDivs(n) {
    var i;

    if (n > x.length) {
      slideIndex = 1
    }
    if (n < 1) {
      slideIndex = x.length
    }
    for (i = 0; i < x.length; i++) {
      x[i].style.display = 'none';
    }
    x[slideIndex - 1].style.display = 'block';

    // Lazy-Loading Image
    var posterImage = $('.mySlides' + sectionIndex + ' .product_img').eq(slideIndex - 1);
    if (!posterImage.attr('src')) {
      posterImage.attr('src', posterImage.data('src'));
    }
  }

  function pop(src) {
    $('.modal-body').html('');
    $('#exampleModalCenter1').modal('show');
    $('.modal-body').append('<img style="width:100%" src="' + src + '" data-dismiss="modal" aria-label="Close"/>');
  }
</script>

